 function initReadMore(visibleReviews) {
  visibleReviews.forEach((reviewItem) => {
    const reviewBody = reviewItem.querySelector(".product-review__body");
    if (!reviewBody) return;

    const textEl = reviewBody.querySelector("p");
    const toggleBtn = reviewBody.querySelector(".product-review__body-read-more");

    if (!textEl || !toggleBtn) return;

    const fullText = textEl.innerText.trim();
    if (fullText.length <= 200) {
      toggleBtn.style.display = 'none';
      return;
    }

    textEl.classList.add("collapsed");
    toggleBtn.style.display = 'inline-block';
    toggleBtn.innerText = "Read more";

    // Prevent multiple event listeners by removing previous ones
    const newToggleBtn = toggleBtn.cloneNode(true);
    toggleBtn.parentNode.replaceChild(newToggleBtn, toggleBtn);

    newToggleBtn.addEventListener("click", () => {
      const isCollapsed = textEl.classList.toggle("collapsed");
      newToggleBtn.innerText = isCollapsed ? "Read more" : "Read less";
    });
  });
}


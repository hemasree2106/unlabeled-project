  const paginationContainer = document.getElementById('custom-review-pagination');
  const reviewsPerPage = parseInt(paginationContainer.dataset.perPage) || 2;

  let filteredReviews = [...allReviews];
  let currentPage = 1;

  // Main function to call sorting + rendering
  function applyReviewSortingAndRender(selected) {
    filteredReviews = [...allReviews];

    switch (selected) {
      case "most-recent":
        filteredReviews.sort((a, b) => {
          const dateA = new Date(a.dataset.postedDate.split("/").reverse().join("/"));
          const dateB = new Date(b.dataset.postedDate.split("/").reverse().join("/"));
          return dateB - dateA;
        });
        break;

      case "highest-rating":
        filteredReviews.sort((a, b) => Number(b.dataset.ratingCount) - Number(a.dataset.ratingCount));
        break;

      case "lowest-rating":
        filteredReviews.sort((a, b) => Number(a.dataset.ratingCount) - Number(b.dataset.ratingCount));
        break;

      case "with-pictures":
        filteredReviews = filteredReviews.filter(review => review.dataset.picturePosted === "true");
        break;

      case "pictures-first":
        filteredReviews.sort((a, b) => (b.dataset.picturePosted === "true") - (a.dataset.picturePosted === "true"));
        break;

      case "most-helpful":
        filteredReviews = filteredReviews.filter(review => review.dataset.mostHelpful === "true");
        break;
    }

    currentPage = 1;
    renderReviews();
  }

function renderReviews() {
  const start = (currentPage - 1) * reviewsPerPage;
  const end = start + reviewsPerPage;

  reviewsContainer.innerHTML = '';

  const reviewsToShow = filteredReviews.slice(start, end);
  reviewsToShow.forEach((review) => {
    reviewsContainer.appendChild(review);
  });

  renderPagination();
  initReadMore(reviewsToShow); // pass only visible reviews
}
//When sorting enable comment the below renderReviews function
 renderReviews()

  function renderPagination() {
    paginationContainer.innerHTML = '';
    const totalPages = Math.ceil(filteredReviews.length / reviewsPerPage);
    console.log(totalPages, "First Element Appended");
    if (totalPages <= 1) return;

    const addBtn = (label, page, extraClass = '') => {
      const btn = document.createElement('a');
      btn.className = `custom-pagination__page jdgm-paginate__page ${extraClass}`.trim();
      btn.setAttribute('data-page', page);
      btn.setAttribute('aria-label', `Page ${page}`);
      btn.setAttribute('tabindex', 0);
      btn.setAttribute('role', 'button');
      btn.innerHTML = label;
      btn.addEventListener('click', () => {
        currentPage = page;
        renderReviews();
